<?xml version='1.0' encoding='utf-8'?>
<widget id="ru.westcom.shop" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Westcom</name>
    <description>
       Торговая мебель. ООО "Вестком"
    </description>
    <author email="info@ghoul.ru" href="http://ghoul.ru">
        ghoul.ru
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access launch-external="yes" origin="tel:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash src="www/img/splash.png" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="phonegap-version" value="cli-6.4.0" />
</widget>
